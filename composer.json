{
    "name": "dianaphp/framework",
    "description": "a php framework aiming for simplicity and clean syntax",
    "keywords": [
        "diana",
        "framework",
        "php"
    ],
    "minimum-stability": "dev",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Diana\\": "src/Diana/",
            "Diana\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-install-cmd": [
            "phpunit"
        ],
        "post-update-cmd": [
            "phpunit"
        ]
    },
    "authors": [
        {
            "name": "Antonio Ianzano",
            "email": "ianzanoan@gmail.com"
        }
    ],
    "require": {
        "php": ">=8.0",
        "filp/whoops": "^2.15",
        "dianaphp/lib": "dev-main"
    },
    "require-dev": {
        "phpunit/phpunit": "^11.2@dev"
    }
}